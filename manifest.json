{
    "name": "ViewHatenaBookmark",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "Hatenaブックマーク見るやつ",
    "icons": {
        "16": "icons/hbicon16.png",
        "48": "icons/hbicon48.png",
        "128": "icons/hbicon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "icons/hbicon19.png"
        },
        "default_title": "ViewHatenaBookmark"
    },
    "permissions": ["<all_urls>", "tabs"],
    "background": {
        "scripts": ["js/jquery-2.2.0.min.js", "js/background.js"]
    }
}
